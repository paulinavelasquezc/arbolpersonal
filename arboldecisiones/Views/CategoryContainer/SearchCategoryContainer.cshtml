@model IEnumerable<arboldecisiones.Models.CategoryContainer>

@{
    ViewBag.Title = "Index";
}

<table class="table">
    <tr>
        <th>
            Acciones
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdateDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Active)
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.Multimedia.Description)
            </th>*@
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <div class="div-images">
                    @Html.ActionLink("Edit", "Edit", new { id = item.CategoryContainerID }, new { @class = "btn-edit" })
                    @Html.ActionLink("Detalle", "Details", new { id = item.CategoryContainerID }, new { @class = "btn-details" })
                    @Html.ActionLink("Eliminar", "Delete", new { id = item.CategoryContainerID }, new { @class = "btn-delete" })
                </div>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
                @*
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Multimedia.Description)
                    </td>*@
        </tr>
    }

</table>